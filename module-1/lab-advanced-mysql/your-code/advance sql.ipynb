  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Challenge 1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "df1 = pd.read_sql_query(\"\"\"\n",
    "SELECT authors.au_id AS `AUTHOR ID`, titles.title_id AS `TITLE_ID`, SUM(titles.price * sales.qty * titles.royalty / 100 * titleauthor.royaltyper / 100) AS sales_royalty\n",
    "    FROM titleauthor \n",
    "    INNER JOIN authors ON titleauthor.au_id=authors.au_id\n",
    "    INNER JOIN titles ON titles.title_id=titleauthor.title_id\n",
    "    INNER JOIN sales ON titles.title_id=sales.title_id\n",
    "    GROUP BY authors.au_id, titles.title_id\n",
    "    ORDER BY sales_royalty DESC\n",
    "    \"\"\", engine)"
   ]
  },
